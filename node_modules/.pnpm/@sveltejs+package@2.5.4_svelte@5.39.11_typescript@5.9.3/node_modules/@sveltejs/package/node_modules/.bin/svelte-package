#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+package@2.5.4_svelte@5.39.11_typescript@5.9.3/node_modules/@sveltejs/package/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+package@2.5.4_svelte@5.39.11_typescript@5.9.3/node_modules/@sveltejs/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+package@2.5.4_svelte@5.39.11_typescript@5.9.3/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+package@2.5.4_svelte@5.39.11_typescript@5.9.3/node_modules/@sveltejs/package/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+package@2.5.4_svelte@5.39.11_typescript@5.9.3/node_modules/@sveltejs/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+package@2.5.4_svelte@5.39.11_typescript@5.9.3/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../svelte-package.js" "$@"
else
  exec node  "$basedir/../../svelte-package.js" "$@"
fi
