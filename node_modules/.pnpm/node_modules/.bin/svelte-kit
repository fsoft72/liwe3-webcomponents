#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fabio/dev/cloudflare/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.45.6_vite@7.2.6__svelte@5.45.6_vite@7.2.6/node_modules/@sveltejs/kit/node_modules:/home/fabio/dev/cloudflare/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.45.6_vite@7.2.6__svelte@5.45.6_vite@7.2.6/node_modules/@sveltejs/node_modules:/home/fabio/dev/cloudflare/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.45.6_vite@7.2.6__svelte@5.45.6_vite@7.2.6/node_modules:/home/fabio/dev/cloudflare/liwe3-webcomponents/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fabio/dev/cloudflare/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.45.6_vite@7.2.6__svelte@5.45.6_vite@7.2.6/node_modules/@sveltejs/kit/node_modules:/home/fabio/dev/cloudflare/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.45.6_vite@7.2.6__svelte@5.45.6_vite@7.2.6/node_modules/@sveltejs/node_modules:/home/fabio/dev/cloudflare/liwe3-webcomponents/node_modules/.pnpm/@sveltejs+kit@2.49.1_@sveltejs+vite-plugin-svelte@5.1.1_svelte@5.45.6_vite@7.2.6__svelte@5.45.6_vite@7.2.6/node_modules:/home/fabio/dev/cloudflare/liwe3-webcomponents/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
