#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/publint@0.2.12/node_modules/publint/lib/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/publint@0.2.12/node_modules/publint/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/publint@0.2.12/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/publint@0.2.12/node_modules/publint/lib/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/publint@0.2.12/node_modules/publint/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/publint@0.2.12/node_modules:/home/fabio/dev/projects/liwe3-webcomponents/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../publint/lib/cli.js" "$@"
else
  exec node  "$basedir/../publint/lib/cli.js" "$@"
fi
