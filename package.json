{
  "name": "liwe3-webcomponents-monorepo",
  "version": "1.1.0",
  "private": true,
  "description": "Monorepo for @liwe3/webcomponents packages",
  "scripts": {
    "build": "turbo run build --filter=\"!@liwe3/demo\"",
    "build:core": "turbo run build --filter=\"@liwe3/webcomponents\"",
    "build:svelte": "turbo run package --filter=\"@liwe3/webcomponents-svelte\"",
    "build:demo": "turbo run build --filter=\"@liwe3/demo\"",
    "build:docs": "pnpm run build:core && mkdir -p docs/source/javascripts && cp packages/core/dist/*.js docs/source/javascripts/",
    "dev": "turbo run dev --filter=\"!@liwe3/demo\"",
    "dev:core": "turbo run dev --filter=\"@liwe3/webcomponents\"",
    "dev:svelte": "turbo run dev --filter=\"@liwe3/webcomponents-svelte\"",
    "dev:demo": "turbo run dev --filter=\"@liwe3/demo\"",
    "check": "turbo run check",
    "clean": "rm -rf packages/*/dist packages/*/.svelte-kit .turbo",
    "clean:all": "rm -rf packages/*/dist packages/*/.svelte-kit packages/*/node_modules packages/*/.vite packages/*/.turbo .turbo .vite node_modules/.cache node_modules/.vite"
  },
  "packageManager": "pnpm@10.19.0",
  "devDependencies": {
    "turbo": "^2.6.1"
  }
}